                },
                "total":{
                    "type":"Integer",
                    "description":"记录总数",
                    "sample":10
                },
                "page":{
                    "type":"Integer",
                    "description":"当前页码",
                    "sample":1
                },
                "limit":{
                    "type":"Integer",
                    "description":"每页条数",
                    "sample":10
