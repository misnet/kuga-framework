{
    "id":"product.propset.list",
    "name":"属性集合列表",
    "description":"取得属性集合列表",
    "namespace":"Kuga\\Api\\Console",
    "method":"props.listPropSets",
    "accessLevel": 1,
    "request":[
        {
            "param":"page",
            "required":false,
            "default":1,
            "name":"页码",
            "type":"Integer",
            "description":"页码"
        },
        {
            "param":"limit",
            "required":false,
            "default":10,
            "type":"Integer",
            "description":"每页显示条数",
            "name":"每页显示条数"
        }
    ],
    "response":{
        "data":{
            "type":"Object",
            "hasSampleFile":true,
            "responseItem": {
                "list":{
                    "type":"Array",
                    "responseItem":{
                        "id":{
                            "type":"Integer",
                            "sample":1,
