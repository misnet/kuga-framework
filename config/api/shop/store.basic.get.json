{
  "id":"store.basic.get",
  "name":"获取店仓",
  "description":"根据ID获取一个门店或仓库",
  "namespace":"Kuga\\Api\\Console",
  "method":"store.getStore",
  "accessLevel": 1,
  "request":[
    {
      "param":"id",
      "required":true,
      "default":0,
      "type":"String",
      "description":"店仓id"
    }
  ],
  "response":{
    "data":{
      "type":"Object",
      "responseItem":{
        "id":{
          "type":"Integer",
          "sample":1,
          "description":"店仓ID"
        },
        "name":{
          "type":"String",
          "sample":"",
          "description":"店仓名称"
        },
        "countryId":{
          "type":"Integer",
          "sample":"",
          "description":"国家ID"
        },
        "provinceId":{
          "type":"Integer",
          "sample":1,
          "description":"省份ID"
        },
        "cityId":{
          "type":"Integer",
          "sample":"",
          "description":"城市ID"
        },
        "address":{
          "type":"String",
          "sample":"",
          "description":"详细地址"
        },
        "countryName":{
          "type":"String",
          "sample":"中国",
          "description":"国家名称"
        },
        "provinceName":{
          "type":"String",
          "sample":"福建省",
          "description":"省份名称"
        },
        "cityName":{
          "type":"String",
          "sample":"厦门市",
          "description":"城市名称"
        },
        "summary": {
          "type":"String",
          "sample":"",
          "description":"描述摘要"
        },
        "disabled": {
          "type":"Integer",
          "sample":"1",
          "description":"禁用或启用，1禁用，0启用"
        },
        "isRetail": {
          "type":"Integer",
          "sample":"1",
          "description":"是否零售，1是，0不是"
        }
      },
      "description":"成功返回true，失败返回false"
    }
  }
}