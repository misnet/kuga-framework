{
  "id":"inventory.sheet.get",
  "name":"取得出入库单",
  "description":"根据单ID取得一个出入库单",
  "namespace":"Kuga\\Api\\Console",
  "method":"inventory.getSheet",
  "accessLevel": 1,
  "request":[
    {
      "param":"id",
      "required":true,
      "default":"",
      "type":"Integer",
      "description":"单据的ID"
    }
  ],
  "response":{
    "data":{
      "type":"Object",
      "description":"单据对象",
      "responseItem":{
        "id":{
          "type":"Integer",
          "sample":1,
          "description":"单据ID"
        },
        "storeId":{
          "type":"Integer",
          "sample":1,
          "description":"单据所属店仓ID"
        },
        "sheetType":{
          "type":"Integer",
          "sample":1,
          "description":"出入单类型，1入库，2出库"
        },
        "sheetTime":{
          "type":"Integer",
          "sample":1546996594,
          "description":"出入库时间"
        },
        "createTime":{
          "type":"Integer",
          "sample":1546996594,
          "description":"创建时间"
        },
        "sheetDesc":{
          "type":"String",
          "sample":"",
          "description":"描述"
        },
        "userId":{
          "type":"Integer",
          "sample":2,
          "description":"操作人员ID"
        },
        "sheetCode":{
          "type":"String",
          "sample":"ABC0982",
          "description":"单号"
        },
        "itemList":{
          "type":"JsonString",
          "description":"出入库明细",
          "responseItem":{
            "id":{
              "type":"Integer",
              "sample":1,
              "description":"SKUID"
            },
            "price":{
              "type":"Float",
              "sample":0.99,
              "description":"SKU单价"
            },
            "cost":{
              "type":"Float",
              "sample":0.99,
              "description":"SKU成本"
            },
            "originalSkuId":{
              "type":"String",
              "sample":"",
              "description":"原厂SKU编号"
            },
            "skuSn":{
              "type":"String",
              "sample":"",
              "description":"SKU条码"
            },
            "productId":{
              "type":"Integer",
              "sample":1,
              "description":"商品ID"
            },
            "skuString":{
              "type":"String",
              "sample":"颜色：红色；尺码：XL",
              "description":"SKU名称"
            },
            "qty":{
              "type":"Integer",
              "sample":200,
              "description":"出入库数"
            },
            "title":{
              "type":"String",
              "sample":"",
              "description":"商品名称"
            },
            "skuJson":{
              "type":"String",
              "sample":"[{\"prop\":\"1\",\"value\":46},{\"prop\":\"2\",\"value\":44}]",
              "description":"SKU属性 JSON格式,prop表示属性，value表示属性值"
            }
          }
        }

      }
    }
  }
}