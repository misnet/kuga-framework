{
  "id":"store.basic.list",
  "name":"店仓列表",
  "description":"根据条件查询店仓列表",
  "namespace":"Kuga\\Api\\Console",
  "method":"store.listStores",
  "accessLevel": 1,
  "request":[
    {
      "param":"page",
      "required":false,
      "default":1,
      "name":"页码",
      "type":"Integer",
      "description":"页码"
    },
    {
      "param":"limit",
      "required":false,
      "default":10,
      "type":"Integer",
      "description":"每页显示条数",
      "name":"每页显示条数"
    },
    {
      "param":"isRetail",
      "required":false,
      "default":-1,
      "type":"Integer",
      "description":"是否零售，-1全部，0不是，1是"
    },
    {
      "param":"keyword",
      "required":false,
      "default":"",
      "type":"String",
      "description":"搜索关键词"
    }
  ],
  "response":{
    "data":{
      "type":"Object",
      "responseItem": {
        "list":{
          "type":"Array",
          "responseItem":{
            "id":{
              "type":"Integer",
              "sample":1,
              "description":"店仓ID"
            },
            "name":{
              "type":"String",
              "sample":"",
              "description":"店仓名称"
            },
            "countryId":{
              "type":"Integer",
              "sample":"",
              "description":"国家ID"
            },
            "provinceId":{
              "type":"Integer",
              "sample":1,
              "description":"省份ID"
            },
            "cityId":{
              "type":"Integer",
              "sample":"",
              "description":"城市ID"
            },
            "address":{
              "type":"String",
              "sample":"",
              "description":"详细地址"
            },
            "countryName":{
              "type":"String",
              "sample":"中国",
              "description":"国家名称"
            },
            "provinceName":{
              "type":"String",
              "sample":"福建省",
              "description":"省份名称"
            },
            "cityName":{
              "type":"String",
              "sample":"厦门市",
              "description":"城市名称"
            },
            "summary": {
              "type":"String",
              "sample":"",
              "description":"描述摘要"
            },
            "disabled": {
              "type":"Integer",
              "sample":"1",
              "description":"禁用或启用，1禁用，0启用"
            },
            "isRetail": {
              "type":"Integer",
              "sample":"1",
              "description":"是否零售，1是，0不是"
            }
          },
          "description":"数据列表"
        },
        "total":{
          "type":"Integer",
          "description":"记录总数",
          "sample":10
        },
        "page":{
          "type":"Integer",
          "description":"当前页码",
          "sample":1
        },
        "limit":{
          "type":"Integer",
          "description":"每页条数",
          "sample":10
        }
      }
    }
  }
}