    "namespace":"Kuga\\Api\\Console",
    "method":"user.userList",
    "accessLevel": 1,
    "request":[
        {
            "param":"page",
            "required":false,
            "default":1,
            "name":"页码",
            "type":"Integer",
            "description":"页码"
        },
        {
            "param":"limit",
            "required":false,
            "default":10,
